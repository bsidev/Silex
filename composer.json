{
  "name": "silex/silex",
  "description": "The PHP micro-framework based on the Symfony Components",
  "keywords": ["microframework"],
  "homepage": "https://silex.symfony.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Fabien Potencier",
      "email": "fabien@symfony.com"
    },
    {
      "name": "Igor Wiedler",
      "email": "igor@wiedler.ch"
    },
    {
      "name": "Sergey Balasov",
      "email": "sbalasov@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "pimple/pimple": "^3.0",
    "symfony/event-dispatcher": "^6.0",
    "symfony/http-foundation": "^6.0",
    "symfony/http-kernel": "^6.0",
    "symfony/routing": "^6.0"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^6.0",
    "symfony/validator": "^6.0",
    "symfony/serializer": "^6.0",
    "symfony/browser-kit": "^6.0",
    "symfony/http-client": "^6.0"
  },
  "replace": {
    "silex/api": "self.version",
    "silex/providers": "self.version"
  },
  "autoload": {
    "psr-4": {"Silex\\": "src/Silex"}
  },
  "autoload-dev": {
    "psr-4": {"Silex\\Tests\\": "tests/Silex/Tests"}
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.3.x-dev"
    }
  }
}
